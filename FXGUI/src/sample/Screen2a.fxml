<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.media.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MainSceneController">
  <children>
      <HBox>
         <children>
            <ListView fx:id="songList" prefHeight="354.0" />
            <ListView fx:id="queueList" blendMode="DARKEN">
               <effect>
                  <Glow />
               </effect>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </ListView>
            <TextArea fx:id="songRequest" prefHeight="130.0" prefWidth="162.0" />
         </children>
      </HBox>
      <Pane fx:id="loadButton" layoutX="-14.0" layoutY="-7.0">
         <children>
            <Button fx:id="skipButton" layoutX="534.0" layoutY="185.0" mnemonicParsing="false" onAction="#skipMethod" text="Skip" />
            <Text fill="#0f1108" layoutX="75.0" layoutY="47.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Song Queue" wrappingWidth="119.0" />
            <Button fx:id="loadButton" layoutX="522.0" layoutY="369.0" mnemonicParsing="false" onAction="#startServer" text="Start Server" />
        <Button layoutX="433.0" layoutY="18.0" mnemonicParsing="false" onAction="#goToScreen3" text="Disk Jockey" />
            <ProgressIndicator fx:id="prog" layoutX="389.0" layoutY="11.0" progress="0.0" />
            <ProgressBar fx:id="progBar" layoutX="281.0" layoutY="17.0" progress="0.0" />
            <Button fx:id="addbtn" layoutX="535.0" layoutY="225.0" mnemonicParsing="false" onAction="#add" text="add" />
            <Button fx:id="removebtn" layoutX="533.0" layoutY="262.0" mnemonicParsing="false" onAction="#remove" text="remove" />
            <Button fx:id="initbtn" layoutX="545.0" layoutY="335.0" mnemonicParsing="false" onAction="#init" text="init" />
            <SplitMenuButton layoutX="14.0" layoutY="6.0" mnemonicParsing="false" text="Menu">
              <items>
                <MenuItem mnemonicParsing="false" text="Logout" />
                <MenuItem mnemonicParsing="false" text="Switch to DJ scene" />
              </items>
            </SplitMenuButton>
        <Button fx:id="refreshButton" layoutX="533.0" layoutY="300.0" mnemonicParsing="false" onAction="#refreshMethod" text="Refresh" />
            <Button fx:id="playButton" layoutX="542.0" layoutY="408.0" mnemonicParsing="false" onAction="#play" text="Play" />
         </children>
      </Pane>
  </children>
</AnchorPane>
