        </HBox>
        <Label fx:id="response" alignment="CENTER" contentDisplay="CENTER" layoutX="236.0" layoutY="284.0" prefHeight="25.0" prefWidth="181.0" textAlignment="JUSTIFY" textFill="RED">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Label id="MusicHostLabel" contentDisplay="CENTER" layoutX="471.0" layoutY="154.0" prefHeight="72.0" prefWidth="285.0" text="Music Host " textAlignment="JUSTIFY">
            <font>
                <Font size="48.0" />
            </font>
        </Label>
