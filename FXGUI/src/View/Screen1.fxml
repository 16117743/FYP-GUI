<AnchorPane id="AnchorPane" minHeight="122.0" prefHeight="599.0" prefWidth="1021.0" stylesheets="@login.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoginSceneController">
