                        <Cursor fx:constant="WAIT" />
                    </cursor>
                </Button>
                <Button fx:id="refreshButton" mnemonicParsing="false" prefHeight="42.0" prefWidth="62.0" text="Refresh" />
                <ProgressIndicator fx:id="prog" progress="0.0" />
            </children>
        </HBox>
        <VBox layoutX="1270.0" layoutY="122.0" prefHeight="224.0" prefWidth="69.0">
            <children>
                <Button fx:id="boolRequest" mnemonicParsing="false" onAction="#setBool1" prefHeight="45.0" prefWidth="72.0" text="OFF" />
                <Button fx:id="boolDJComment" mnemonicParsing="false" onAction="#setBool2" prefHeight="45.0" prefWidth="69.0" text="OFF" />
                <Button fx:id="boolSkip" mnemonicParsing="false" onAction="#setBool3" prefHeight="52.0" prefWidth="70.0" text="OFF" />
                <Button fx:id="boolEcho" mnemonicParsing="false" onAction="#setBool4" prefHeight="47.0" prefWidth="69.0" text="OFF" />
                <Button fx:id="boolBlob" mnemonicParsing="false" onAction="#setBool5" prefHeight="48.0" prefWidth="69.0" text="OFF" />
            </children>
        </VBox>
        <MediaView fx:id="mediaView" fitHeight="200.0" fitWidth="400.0" layoutX="443.0" layoutY="14.0">
            <mediaPlayer>
                <MediaPlayer autoPlay="false">
                    <media>
                        <Media source="http://download.oracle.com/otndocs/products/javafx/oow2010-2.flv" />
                    </media>
                </MediaPlayer>
            </mediaPlayer>
        </MediaView>
        <Slider fx:id="timeSlider" layoutX="534.0" layoutY="212.0" prefHeight="14.0" prefWidth="218.0" />
        <ProgressBar fx:id="songProgressBar" layoutX="853.0" layoutY="194.0" prefWidth="200.0" progress="0.0" />
        <Slider fx:id="volumeSlider" layoutX="383.0" layoutY="61.0" orientation="VERTICAL" />
        <Label fx:id="timeLabel" layoutX="896.0" layoutY="152.0" text="Label" />
