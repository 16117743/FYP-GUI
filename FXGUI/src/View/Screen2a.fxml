            </children>
        </HBox>
        <HBox layoutX="1126.0" layoutY="14.0" prefHeight="47.0" prefWidth="172.0">
            <children>
                <Button mnemonicParsing="false" onAction="#goToScreen3" prefHeight="52.0" prefWidth="82.0" text="Disk Jockey" />
                <Button fx:id="javascript" mnemonicParsing="false" onAction="#goToScreen1" prefHeight="53.0" prefWidth="86.0" text="logout" />
            </children>
        </HBox>
        <HBox layoutX="42.0" layoutY="631.0" prefHeight="38.0" prefWidth="208.0">
            <children>
                <Button fx:id="serverButton" contentDisplay="CENTER" mnemonicParsing="false" onAction="#startServer" prefHeight="36.0" prefWidth="96.0" text="OFF" textOverrun="CLIP">
