<AnchorPane id="AnchorPane" fx:id="theRoot" prefHeight="716.0" prefWidth="1337.0" style="-fx-background-color: pink;" stylesheets="@MainSceneStyle.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MainSceneController">
    <children>
        <HBox layoutX="353.0" layoutY="234.0" prefHeight="398.0" prefWidth="600.0">
            <children>
                <ListView fx:id="queueList" blendMode="DARKEN" prefHeight="318.0" prefWidth="193.0">
                    <effect>
                        <Glow />
                    </effect>
                    <cursor>
                        <Cursor fx:constant="HAND" />
                    </cursor>
                </ListView>
                <ListView fx:id="songList" prefHeight="400.0" prefWidth="207.0" />
                <ListView fx:id="dJComments" prefHeight="372.0" prefWidth="205.0" />
            </children>
        </HBox>
        <HBox layoutX="443.0" layoutY="650.0" prefHeight="64.0" prefWidth="453.0">
            <children>
                <Button fx:id="playButton" mnemonicParsing="false" onAction="#iPlay" prefHeight="47.0" prefWidth="55.0" text="Pause" />
                <Button fx:id="skipButton" mnemonicParsing="false" onAction="#iSkip" prefHeight="47.0" prefWidth="55.0" text="Skip" />
                <Button fx:id="initbtn" mnemonicParsing="false" onAction="#init" prefHeight="47.0" prefWidth="59.0" text="init" />
